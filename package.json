{
  "name": "vscode-expo-theme",
  "displayName": "Expo Theme",
  "version": "0.0.0",
  "description": "TODO",
  "keywords": [
    "TODO"
  ],
  "license": "MIT",
  "author": "expo",
  "homepage": "https://github.com/bycedric/vscode-expo-theme",
  "bugs": {
    "url": "https://github.com/bycedric/vscode-expo-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bycedric/vscode-expo-theme"
  },
  "bin": "./src/bin.ts",
  "scripts": {
    "dev": "nodemon --watch ./src --exec ts-node ./src/scripts/build-themes.ts",
    "build": "ts-node ./src/scripts/build-themes.ts",
    "clean": "git clean -xdf ./build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo/styleguide-base": "1.0.0-alpha.2",
    "arg": "^5.0.2",
    "hsl-to-hex": "^1.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^16.11.56",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vscode/extension-telemetry": "^0.6.2",
    "@vscode/vsce": "^2.17.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-universe": "^11.1.1",
    "nodemon": "^2.0.20",
    "ovsx": "^0.8.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": "universe/node",
    "ignorePatterns": [
      "node_modules/**",
      "build/**"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "engines": {
    "vscode": "*"
  },
  "contributes": {
    "themes": [
      {
        "label": "Expo Dark",
        "uiTheme": "vs-dark",
        "path": "./build/expo-dark.json"
      }
    ]
  }
}
